<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" spacing="10.0">

        <HBox alignment="CENTER" prefHeight="124.0" prefWidth="800.0" spacing="10.0">
            <Button fx:id="backToProfileButton" onAction="#handleBackToProfileButtonAction" prefHeight="68.0" prefWidth="92.0" text="Back to Profile" />
         <Label prefHeight="30.0" prefWidth="607.0" style="-fx-font-size: 20px;" text="esm lplatform w logo" />
        </HBox>

        <VBox alignment="CENTER" spacing="10.0">
            <TextField fx:id="postTextField" prefHeight="26.0" prefWidth="629.0" promptText="What's on your mind?" />
            <Button fx:id="browseButton" onAction="#handleBrowseButtonAction" prefHeight="26.0" prefWidth="103.0" text="Browse" />
            <Button fx:id="postButton" onAction="#handlePostButtonAction" prefHeight="26.0" prefWidth="839.0" text="Post" />
            <Label prefHeight="30.0" prefWidth="114.0" style="-fx-font-size: 20px;" text="what's new" />
            <ListView fx:id="feedListView" prefHeight="600.0" prefWidth="989.0" />
        </VBox>
        <HBox alignment="CENTER" spacing="10.0" />
    </VBox>
</AnchorPane>
